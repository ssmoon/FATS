@{
    ViewBag.Title = "转账支票数据设置";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model  FATS.Models.TransferCheck

@section Styles {
    <link href="@Url.Content("~/Content/Settings/SettingCommon.css")" rel="Stylesheet" type="text/css" />
}

@section Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/Settings/InitT1RoutineData.js")" ></script>
}

<div class='pagetitle'>@ViewBag.Title</div>
<div id="editcontainer">
    @Html.Hidden("PostUrl", "/Settings/" + ViewContext.Controller.ValueProvider.GetValue("controller").RawValue + "/SaveT1RoutineData")
 @{ Html.RenderPartial("R_TransferCheck", Model); }

    <div id="actbar" class="actbar">
        <a class="easyui-linkbutton l-btn" iconcls="icon-save" href="javascript:SaveNodeData()">保存</a>
        <a class="easyui-linkbutton l-btn" iconcls="icon-back" href="javascript:BackToRoutine()">返回</a>
    </div>  
</div>
