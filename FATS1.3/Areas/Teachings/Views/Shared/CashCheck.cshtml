@model FATS.Areas.Teachings.Models.V_CashCheck

<table class="table table-bordered table-condensed" cellspacing="8" style="margin: 20px 0px;">
    <tr>
        <td colspan="6">
            <div class="billtitle"><span class="bankname">@Model.BankName</span>现金支票</div>
        </td>
    </tr>
    <tr>
        <td class="titlecol">出票日期</td>
        <td>@{ FATS.DataDefine.CommFunctions.FormatDateTimeToNormal(Model.TimeMark); }</td>
        <td class="titlecol">付款行名称</td>
        <td>@Model.BankName</td>
    </tr>
    <tr>
        <td class="titlecol">收款人</td>
        <td>@Model.ClientName</td>
        <td class="titlecol">出票人账号</td>
        <td>@Model.ClientAcc</td>
    </tr>    
    <tr>
        <td class="titlecol">金额</td>
        <td colspan="3">@{ FATS.DataDefine.CommFunctions.FormatMoneyToString(Model.EntryAmount); }</td>
    </tr>

</table>