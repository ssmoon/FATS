@{
    ViewBag.Title = "银行汇票";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model  FATS.Models.BankDraft


<div id="maincontainer">
    <div class="page-header page-header-fix">
        <h3>@ViewBag.NodeName<small>@ViewBag.RoutineName</small></h3>
    </div>
    @{  Html.RenderPartial("CaseHeaderDesc", @ViewData["CaseDesc"]); }
    @Html.HiddenFor(info => info.TchRoutineID)
    <input type="hidden" id="TchNodeID" value="@ViewBag.TchNodeID" />

    @{ Html.RenderPartial("BankDraft", FATS.BusinessObject.Converters.BankDraftConverter.N7_BankDraft(Model)); }
    @{ Html.RenderAction("SubjectFiller", "CommonNode", new { rid = Model.TchRoutineID, nid = ViewBag.TchNodeID }); }
</div>

@{  Html.RenderPartial("Teaching1Navigator");}